<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:background="@drawable/images_4"
		android:orientation="vertical">
		<ImageView
			android:id="@+id/imageview3"
			android:layout_width="wrap_content"
			android:layout_height="80dp"
			android:src="@drawable/images_10"
			android:scaleType="fitXY" />
		<ImageView
			android:id="@+id/imageview1"
			android:layout_width="200dp"
			android:layout_height="100dp"
			android:layout_marginTop="5dp"
			android:src="@drawable/images_21"
			android:scaleType="fitXY"
			android:layout_gravity="center_horizontal" />
		<LinearLayout
			android:id="@+id/linear5"
			android:layout_width="match_parent"
			android:layout_height="80dp"
			android:layout_marginTop="10dp"
			android:padding="8dp"
			android:background="@drawable/images_14"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:layout_gravity="center_horizontal">
			<Button
				android:id="@+id/new_account_requests"
				android:layout_width="200dp"
				android:layout_height="60dp"
				android:layout_marginTop="4dp"
				android:padding="8dp"
				android:background="@drawable/images_27"
				android:textSize="12sp"
				android:textColor="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear7"
			android:layout_width="match_parent"
			android:layout_height="80dp"
			android:layout_marginTop="5dp"
			android:padding="8dp"
			android:background="@drawable/images_14"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:layout_gravity="center_horizontal">
			<Button
				android:id="@+id/all_student_account"
				android:layout_width="200dp"
				android:layout_height="60dp"
				android:layout_marginTop="4dp"
				android:padding="8dp"
				android:background="@drawable/images_29"
				android:textSize="12sp"
				android:textColor="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear8"
			android:layout_width="match_parent"
			android:layout_height="80dp"
			android:layout_marginTop="5dp"
			android:padding="8dp"
			android:background="@drawable/images_14"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:layout_gravity="center_horizontal">
			<Button
				android:id="@+id/all_teacher_account"
				android:layout_width="200dp"
				android:layout_height="60dp"
				android:layout_marginTop="4dp"
				android:padding="8dp"
				android:background="@drawable/images_31"
				android:textSize="12sp"
				android:textColor="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear11"
			android:layout_width="match_parent"
			android:layout_height="80dp"
			android:layout_marginTop="5dp"
			android:padding="8dp"
			android:background="@drawable/images_14"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:layout_gravity="center_horizontal">
			<Button
				android:id="@+id/all_deactivated_ac"
				android:layout_width="200dp"
				android:layout_height="60dp"
				android:layout_marginTop="4dp"
				android:padding="8dp"
				android:background="@drawable/image1"
				android:textSize="12sp"
				android:textColor="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear10"
			android:layout_width="match_parent"
			android:layout_height="80dp"
			android:layout_marginTop="5dp"
			android:padding="8dp"
			android:background="@drawable/images_14"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:layout_gravity="center_horizontal">
			<Button
				android:id="@+id/add_update_subject"
				android:layout_width="200dp"
				android:layout_height="60dp"
				android:layout_marginTop="4dp"
				android:padding="8dp"
				android:background="@drawable/image"
				android:textSize="12sp"
				android:textColor="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear9"
			android:layout_width="match_parent"
			android:layout_height="80dp"
			android:layout_marginTop="5dp"
			android:padding="8dp"
			android:background="@drawable/images_14"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:layout_gravity="center_horizontal">
			<Button
				android:id="@+id/logout"
				android:layout_width="200dp"
				android:layout_height="60dp"
				android:layout_marginTop="4dp"
				android:padding="8dp"
				android:background="@drawable/images_30"
				android:textSize="12sp"
				android:textColor="#000000" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
